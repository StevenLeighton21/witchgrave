<% provide(:title, 'All Campaigns') %>
<div class="col-xs-12 .col-sm-6 .col-md-8", id="content">  
<h1>All Campaigns</h1>

<%= will_paginate %>

<ul class="campaigns">
  <% @campaigns.each do |campaign| %>
    <li>
      
      <%= link_to campaign.name, campaign %>
      <p> Created by: <%= user_name_with_id(campaign.user_id) %> on: <%= campaign.created_at %> </p>
     </li>
  <% end %>
</ul>

<%= will_paginate %>

</div>